{
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./"
                ],
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ]
    ],
    "env": {
        "development": {
            "presets": [
                "next/babel"
            ]
        },
        "production": {
            "presets": [
                "next/babel"
            ]
        },
        "test": {
            "plugins": [
                [
                    "module-resolver",
                    {
                        "root": [
                            "./"
                        ],
                        "extensions": [
                            ".js",
                            ".jsx"
                        ]
                    }
                ]
            ],
            "presets": [
                [
                    "next/babel",
                    {
                        "preset-env": {
                            "modules": "commonjs"
                        }
                    }
                ]
            ]
        }
    }
}